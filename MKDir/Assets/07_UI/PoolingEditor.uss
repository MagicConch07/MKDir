.custom-label {
    font-size: 20px;
    -unity-font-style: bold;
    color: rgb(68, 138, 255);
}

.pool-btn {
    flex-grow: 1;
    background-color: rgba(80, 150, 80, 0.71);
    transition-duration: 0.3s;
}

.enum-btn {
    flex-grow: 1;
    transition-duration: 0.3s;
    background-color: rgba(160, 170, 0, 0.71);
}

.pool-btn:hover {
    background-color: rgb(80, 150, 80);
}

.enum-btn:hover {
    background-color: rgb(160, 170, 0);
}
